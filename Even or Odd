import java.io.*;
import java.util.*;
class num
{
public static void main(String args[])
{
int n,even,odd;
Scanner t=new scanner(System.in);
System.out.println("enter the number");
n=t.nextInt();
if(n%2==0)
{ 
System.out.println("number is even:"+even); 
}
else
{
System.out.println("number is even:"+odd); 
}   
}
}   
